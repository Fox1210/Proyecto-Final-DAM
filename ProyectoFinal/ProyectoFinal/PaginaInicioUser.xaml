<Page
    x:Class="ProyectoFinal.PaginaInicioUser"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ProyectoFinal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="JavaOption" Content="Java" Margin="200,200,0,0" VerticalAlignment="Top" FontSize="30" Width="200" Click="JavaOption_Click"/>
        <Button x:Name="SqlOption_Copy" Content="SQL" Margin="450,200,0,0" VerticalAlignment="Top" FontSize="30" Width="200"/>
        <Button x:Name="configuracion" Content="Configuración" Margin="966,75,0,0" VerticalAlignment="Top" FontSize="30" Width="250" Click="configuracion_Click"/>
        <Button x:Name="LogOutBtn" Content="Log Out" Margin="184,75,0,0" VerticalAlignment="Top" FontSize="30" Width="250" Click="logOut_Click"/>
    </Grid>
</Page>
